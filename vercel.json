{
    "version": 2,
    "builds": [
        {
            "src": "web_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "methods": ["POST"],
            "dest": "web_app.py"
        },
        {
            "src": "/(.*)",
            "dest": "web_app.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
} 