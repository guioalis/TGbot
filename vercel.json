{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/vercel.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/vercel.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "PYTHON_VERSION": "3.9",
        "LOG_LEVEL": "info"
    }
} 